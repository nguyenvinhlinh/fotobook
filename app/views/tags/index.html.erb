<p id="notice"><%= notice %></p>
<table border="1">
    <thead>
        <tr>
            <th>ID</th>
            <th>Tag</th>
            <th colspan="3"></th>
        </tr>
    </thead>
    
    <tbody>
        <% @tags.each do |tag| %>
            <tr>
                <td><%= tag.id %></td>
                <td><a href="/tags/<%=tag.tag %>"> <%= tag.tag %></a> </td>
            </tr>
        <% end %>
    </tbody>
</table>
<br>

<%= link_to 'New Tags', new_tag_path %>


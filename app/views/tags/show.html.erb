<p id="notice"><%= notice %></p>

<p>
  <strong>Tags:</strong>
  <%= @tag.tag %>

  <% @tag.pictures.each do |f| %>
      <img class="img-responsive" src="<%= f.url%>" />
  <% end %>
</p>
<%= link_to 'Destroy', {controller: "tags", action: "destroy"}, method: :delete,
  id:@tag%>
<%= link_to 'Back', {controller: "tags", action: "index"}%>

<div class="container" >
  <p >
    <strong>Tags:</strong> <%= @tag.tag %>
  </p>
  <div class="grid">
    <div class="grid-sizer"></div>
    <% @pictures.each do |f| %>
      <div class="grid-item">
        <img src="<%= f.url %>" href="<%= f.url %>"  hrefto="/pictures/<%= f.id %>" rel="pictures" />
      </div>
    <% end %>
  </div>
  <button id="loadmore-button" class="btn btn-primary btn-lg active">
    Load More Image
  </button>
</div>

<%= javascript_include_tag 'tag_show'%>
